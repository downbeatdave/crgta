<h1>Listing profiles</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Contact</th>
    <th>Email</th>
    <th>Industry</th>
    <th>Services</th>
    <th>Street</th>
    <th>Street 2</th>
    <th>City</th>
    <th>State</th>
    <th>Zipcode</th>
    <th>County</th>
    <th>Phone</th>
    <th>Website</th>
  </tr>

<% @profiles.each do |profile| %>
  <tr>
    <td><%=h profile.name %></td>
    <td><%=h profile.description %></td>
    <td><%=h profile.contact %></td>
    <td><%=h profile.email %></td>
    <td><%=h profile.industry %></td>
    <td><%=h profile.services %></td>
    <td><%=h profile.street %></td>
    <td><%=h profile.street_2 %></td>
    <td><%=h profile.city %></td>
    <td><%=h profile.state %></td>
    <td><%=h profile.zipcode %></td>
    <td><%=h profile.county %></td>
    <td><%=h profile.phone %></td>
    <td><%=h profile.website %></td>
    <td><%= link_to 'Show', profile %></td>
    <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
    <td><%= link_to 'Destroy', profile, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New profile', new_profile_path %>